{
    "manifest_version" : 2,
    "name" : "Cow  Extension",
    "version" : "1.0",
    "web_accessible_resources": [
        "cows/*.jpg"
    ],
    "content_scripts":[

        {
            "matches" : [
                "<all_urls>"
            ],

            "js": ["cow.js"]

        }
        
    ],
    "browser_action":{
        
    },
    
    "background":{
        "scripts":["background.js"]
    }
    
}
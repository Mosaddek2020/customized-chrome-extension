{
    "manifest_version" : 2,
    "name" : "My Extension",
    "version" : "1.0",
    "content_scripts":[

        {
            "matches" : [
                "<all_urls>"
            ],

            "js": ["content.js"]

        }
       
        
    ],

    "browser_action":{
        "default_icon":"icon.png"
    },
    
    "background":{
        "scripts":["background.js"]
    }
    
    
}